@model IEnumerable<ApplicationTEST.Models.Generer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.poste_domande)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.domaine_activite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.photo_profil)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.salaire_min)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.poste_domande)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.domaine_activite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.photo_profil)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.salaire_min)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_generer">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_generer">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_generer">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
